<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
  ui:generateLocales="default"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style type="com.totalchange.discodj.web.client.views.impl.MediaWidget.Style">
    .disabled {
      color: #888;
    }
  </ui:style>

  <g:HTMLPanel ui:field="containerPanel">
    <ui:msg><g:InlineLabel ui:field="titleLabel" /> by <g:InlineLabel ui:field="artistLabel" /></ui:msg>
    <g:InlineLabel ui:field="alreadyPlayedLabel" visible="false">
      <ui:msg>(already played or already in the queue)</ui:msg>
    </g:InlineLabel>
  </g:HTMLPanel>
</ui:UiBinder>